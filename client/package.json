    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "interactive-lint": "eslint-interactive ./src",
    "format": "yarn format:write",
    "format:check": "prettier --check \"**/*.{ts,tsx,json}\" --cache",
